<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml>

  <category>
    <pattern>HELLO</pattern>
    <template>Hi!</template>
  </category>

  <category>
    <pattern>TEST</pattern>
    <template>Success!</template>
  </category>
  
  <category>
    <pattern>RANDOM</pattern>
    <template>
      <random>
        <li>Badger</li>
        <li>Shenanigans</li>
        <li>Perriwinkle</li>
        <li>Slalom</li>
        <li>Schnapps</li>
      </random>
    </template>
  </category>

  <category>
    <pattern>I LIKE *</pattern>
    <template>Yeah, <star /> is alright, I guess.</template>
  </category>

  <category>
    <pattern>DO YOU?</pattern>
    <that>YEAH, * IS ALRIGHT, I GUESS.</that>
    <template>No, <thatstar /> sucks.</template>
  </category>
  
  <category>
    <pattern>REPEAT *</pattern>
    <template>You just said "<input index="1" />".</template>
  </category>

  <category>
    <pattern>SR *</pattern>
    <template><sr /></template>
  </category>

  <category>
    <pattern>TWOSTAR * AND *</pattern>
    <template>Two things: <star index="1" /> and <star index="2" /></template>
  </category>

  <category>
      <pattern>WHY</pattern>
      <that>BECAUSE.</that>
      <template>I don't have all the answers.</template>
  </category>
  
  <category>
      <pattern>WHY</pattern>
      <template>Because.</template>
  </category>

  <category>
      <pattern>MY NAME IS *</pattern>
      <template>Hello, <star /><set name="name"><star /></set></template>
  </category>

  <category>
      <pattern>WHAT'S *</pattern>
      <template><srai>WHAT IS <star /></srai></template>
  </category>

  <category>
    <pattern>WHAT IS YOUR NAME?</pattern>
    <template>My name is <bot name="name" />.</template>
  </category>

  <category>
    <pattern>WHAT IS MY NAME?</pattern>
    <template>You are <get name="name" />.</template>
  </category>

  <category>
    <pattern>WHAT IS THAT?</pattern>
    <template><that /></template>
  </category>

  <category>
    <pattern>WHAT IS *</pattern>
    <template>I don't know what <star /> is.</template>
  </category>

  <category>
    <pattern>SET FOO BAR</pattern>
    <template>Foo has been set to "bar". <set name="foo">bar</set></template>
  </category>

  <category>
    <pattern>SET FOO *</pattern>
    <template>Foo has been set to <star /><set name="foo"><star /></set></template>
  </category>

  <category>
    <pattern>GET FOO</pattern>
    <template>Foo is: <get name="foo"></get></template>
  </category>

  <category>
    <pattern>URL</pattern>
    <template>My website is <bot name="website" /></template>
  </category>

  <category>
    <pattern>TAKE *</pattern>
    <template>
      <think>
        <inventory action="swap"><star /></inventory>
      </think>
      Okay, took <star /> and dropped <get name="last_dropped" default="nothing"></get>
    </template>
  </category>

  <category>
    <pattern>INVENTORY</pattern>
    <template><inventory action="list" /></template>
  </category>
</aiml>
