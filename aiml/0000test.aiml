<?xml version="1.0" encoding="utf-8"?>
<aiml>
  <category>
    <pattern>UPPERCASE *</pattern>
    <template><uppercase><star /></uppercase></template>
  </category>

  <category>
    <pattern>LOWERCASE *</pattern>
    <template><lowercase><star /></lowercase></template>
  </category>

  <category>
    <pattern>FORMAL *</pattern>
    <template><formal><star /></formal></template>
  </category>

  <category>
    <pattern>SENTENCE *</pattern>
    <template><sentence><star /></sentence></template>
  </category>

  <category>
    <pattern>PERSON *</pattern>
    <template><person><star /></person></template>
  </category>

  <category>
    <pattern>PERSONSHORT *</pattern>
    <template><person /></template>
  </category>

  <category>
    <pattern>PERSON2 *</pattern>
    <template><person2><star /></person2></template>
  </category>

  <category>
    <pattern>PERSON2SHORT *</pattern>
    <template><person2 /></template>
  </category>

  <category>
    <pattern>GENDER *</pattern>
    <template><gender><star /></gender></template>
  </category>

  <category>
    <pattern>GENDERSHORT *</pattern>
    <template><gender /></template>
  </category>

  <category>
    <pattern>SIZE</pattern>
    <template>I recognize <size /> sentences.</template>
  </category>

  <category>
    <pattern>VERSION</pattern>
    <template><version /></template>
  </category>

  <category>
    <pattern>TEST</pattern>
    <template>Success!</template>
  </category>
  
  <category>
    <pattern>RANDOM</pattern>
    <template>
      <random>
        <li>Badger</li>
        <li>Shenanigans</li>
        <li>Perriwinkle</li>
        <li>Slalom</li>
        <li>Schnapps</li>
      </random>
    </template>
  </category>

  <category>
    <pattern>I LIKE *</pattern>
    <template>Yeah, <star /> is alright, I guess.</template>
  </category>

  <category>
    <pattern>DO YOU?</pattern>
    <that>YEAH, * IS ALRIGHT, I GUESS.</that>
    <template>No, <thatstar /> sucks.</template>
  </category>

  <category>
    <pattern>REPEAT *</pattern>
    <template>You just said "<input index="1" />".</template>
  </category>

  <category>
    <pattern>SR *</pattern>
    <template><sr /></template>
  </category>

  <category>
    <pattern>TWOSTAR * AND *</pattern>
    <template>Two things: <star index="1" /> and <star index="2" /></template>
  </category>

  <category>
      <pattern>WHY</pattern>
      <that>BECAUSE.</that>
      <template>I don't have all the answers.</template>
  </category>
  
  <category>
      <pattern>WHY</pattern>
      <template>Because.</template>
  </category>

  <category>
      <pattern>WHAT'S *</pattern>
      <template><srai>WHAT IS <star /></srai></template>
  </category>

  <category>
    <pattern>WHAT IS THAT?</pattern>
    <template><that /></template>
  </category>

  <category>
    <pattern>WHAT IS *</pattern>
    <template>I don't know what <star /> is.</template>
  </category>

  <category>
    <pattern>SET FOO BAR</pattern>
    <template>Foo has been set to "bar". <set name="foo">bar</set></template>
  </category>

  <category>
    <pattern>SET FOO *</pattern>
    <template>Foo has been set to <star /><set name="foo"><star /></set></template>
  </category>

  <category>
    <pattern>GET FOO</pattern>
    <template>Foo is: <get name="foo"></get></template>
  </category>
</aiml>
